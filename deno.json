{
  "imports": {
    "hono": "jsr:@hono/hono@^4.7.9"
  },
  "tasks": {
    "start": "deno run --allow-net --allow-read main.ts",
    "dev": "deno run --watch --allow-net --allow-read main.ts"
  },
  "compilerOptions": {
    "jsx": "precompile",
    "jsxImportSource": "hono/jsx"
  },
  "deploy": {
    "project": "elf-api",
    "exclude": ["**/node_modules", "**/.git"]
  }
}